{
  "name": "material-design",
  "version": "1.0.0",
  "description": "Material Design Components for the GCdocs mobile project",
  "main": "index.js",
  "scripts": {
    "build": "webpack -p",
    "start": "webpack-dev-server --mode=development",
    "test": "jest --verbose",
    "test-behaviour": "npx cucumber-js ./features/behaviour/*.feature --require {./features/**/*.js,./node_modules/cucumber-puppeteer/features/**/*.js} --format ./node_modules/cucumber-puppeteer/features/support/util/PrettyFormatter",
    "test-visual-a11y": "npx cucumber-js ./features/visual/*.feature --require {./features/**/*.js,./node_modules/cucumber-puppeteer/features/**/*.js} --format ./node_modules/cucumber-puppeteer/features/support/util/PrettyFormatter"
  },
  "author": "",
  "license": "ISC",
  "jest": {
    "collectCoverage": true
  },
  "dependencies": {
    "@babel/core": "^7.4.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@material/button": "^3.0.0",
    "@material/dialog": "^3.0.0",
    "@material/drawer": "^3.0.0",
    "@material/ripple": "^3.0.0",
    "@material/snackbar": "^3.0.0",
    "@material/theme": "^3.0.0",
    "autoprefixer": "^9.6.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.0.0",
    "extract-loader": "^3.1.0",
    "file-loader": "^4.0.0",
    "material-components-web": "^3.0.0",
    "material-icons": "^0.3.1",
    "node-sass": "^4.13.1",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "webpack": "^4.35.2",
    "webpack-cli": "^3.3.5"
  },
  "devDependencies": {
    "cucumber-puppeteer": "^1.0.12",
    "jest": "^24.8.0",
    "mock-xmlhttprequest": "^4.4.1",
    "webpack-dev-server": "^3.7.2"
  }
}
